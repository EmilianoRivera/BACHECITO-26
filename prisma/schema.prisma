generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NombreUsuario {
  nomu_id  Int @id @default(autoincrement())
  nomu_nombre String
  nomu_appat String
  nomu_apmat String
}
